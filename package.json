{
  "name": "VSA-App",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "security-checks": "git-secrets --scan && git log -p | scanrepo",
    "generate-env": "node ./config/scripts/replace.env",
    "preionic:serve": "npm run-script generate-env --dev",
    "ionic:serve": "ionic-app-scripts serve",
    "prebuild-ios": "npm run-script generate-env --dev",
    "build-ios": "ionic-app-scripts build --target cordova --platform ios --prod --aot --minifyjs -- minifycss --optimizejs",
    "test": "karma start ./test-config/karma.conf.js",
    "test-watch": "karma start ./test-config/karma.conf.js --watch",
    "test-coverage": "karma start ./test-config/karma.conf.js --coverage",
    "e2e": "npm run e2e-update && npm run e2e-test",
    "e2e-test": "protractor ./test-config/protractor.conf.js",
    "e2e-update": "webdriver-manager update --standalone false --gecko false"
  },
  "dependencies": {
    "@angular/animations": "^5.2.11",
    "@angular/common": "^5.2.11",
    "@angular/compiler": "^5.2.11",
    "@angular/compiler-cli": "^5.2.11",
    "@angular/core": "^5.2.11",
    "@angular/forms": "^5.2.11",
    "@angular/http": "^5.2.11",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@angular/platform-server": "^5.2.11",
    "@angular/router": "^5.2.11",
    "@ionic-native/call-number": "^4.15.0",
    "@ionic-native/camera": "^4.15.0",
    "@ionic-native/core": "~4.12.0",
    "@ionic-native/http": "^4.15.0",
    "@ionic-native/in-app-browser": "^4.15.0",
    "@ionic-native/open-native-settings": "^4.16.0",
    "@ionic-native/social-sharing": "^4.15.0",
    "@ionic-native/splash-screen": "~4.12.0",
    "@ionic-native/status-bar": "~4.12.0",
    "@ionic/storage": "^2.2.0",
    "amazon-cognito-identity-js": "^1.29.0",
    "aws-sdk": "2.177.0",
    "call-number": "^1.0.1",
    "cordova-ios": "4.5.5",
    "cordova-open-native-settings": "^1.5.1",
    "cordova-plugin-advanced-http": "^1.9.0",
    "cordova-plugin-camera": "^4.0.3",
    "cordova-plugin-device": "^1.1.4",
    "cordova-plugin-file": "^5.0.0",
    "cordova-plugin-inappbrowser": "^2.0.1",
    "cordova-plugin-ionic-webview": "^1.2.1",
    "cordova-plugin-splashscreen": "^4.0.3",
    "cordova-plugin-whitelist": "^1.3.1",
    "cordova-plugin-x-socialsharing": "^5.4.1",
    "cordova-sqlite-storage": "^2.5.0",
    "es6-promise-plugin": "^4.1.0",
    "ionic-angular": "^3.9.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "mx.ferreyra.callnumber": "0.0.2",
    "rxjs": "^5.5.12",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.4.8",
    "@ionic/app-scripts": "3.2.0",
    "@types/jasmine": "^2.5.41",
    "angular2-template-loader": "^0.6.2",
    "html-loader": "^0.5.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^2.5.3",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "null-loader": "^0.1.1",
    "protractor": "^5.1.1",
    "ts-loader": "^3.0.3",
    "ts-node": "^3.0.2",
    "typescript": "^2.9.2"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-device": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-whitelist": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-advanced-http": {},
      "cordova-plugin-x-socialsharing": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-camera": {},
      "mx.ferreyra.callnumber": {},
      "cordova-sqlite-storage": {},
      "call-number": {},
      "cordova-open-native-settings": {}
    },
    "platforms": [
      "ios"
    ]
  }
}
